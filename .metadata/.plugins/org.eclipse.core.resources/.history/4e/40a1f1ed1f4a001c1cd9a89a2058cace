package com.app.user.model;

import java.util.Date;

import com.app.enums.BloodGroup;
import com.app.model.Audit;
import com.app.login.model.Login;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class User extends Audit {
	
	private static final long serialVersionUID = -2296432970406140566L;

	private int userId;

	private String firstName;
	
	private String middleName;
	
	private String lastName;
	
	private Date dob;
	
	private String mobileNumber;
	
	private String email;
	
	private Enum<BloodGroup> bloodGroup;
	
	private Login login;
	
	
}
